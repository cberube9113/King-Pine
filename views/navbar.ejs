<div class="navbar">
  <div class="navbar-inner">
    <a class="brand" href="/home">Chirper</a>
    <ul class="nav">
      <li><a href="/connect">Connect</a></li>
      <li><a href="/discover">Discover</a></li>
      <li><a href="/me">Me</a></li>
    </ul>
    <div style="float: right; margin-top: 10px; margin-right:10px;">
      <% if(user != undefined) { %>
        <span class="identifier">Logged in: </span><%= user %>
          <input type="button" class="btn" id="logout_btn" value="Log Out" style="margin-top: -5px;" />
      <% } %>
    
      <% if(user == undefined) { %>
        <input type="button" class="btn" id="login_btn" value="Sign Up or Log In!" style="margin-top: -5px;" />
      <% } %>
    </div>
  </div>
</div>

<div>
  <input type="text" name="search" placeholder="Search for User" class=".search-query" id="user_search_box" autocomplete="off">
  <div class="autocomplete" id="user_autocomplete_box">
    <ul>
    </ul>
  </div>
</div>
<div style="clear:both;"></div>

<script type="text/javascript">
  $("#logout_btn").click(function() {
    window.location = "/logout";
  });

  $("#login_btn").click(function() {
    window.location = "/";
  });


</script>